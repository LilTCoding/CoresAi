{
  "project": {
    "name": "CoresAI",
    "version": "4.1.0",
    "description": "Advanced AI System with Multi-Backend Architecture",
    "author": "Torey",
    "created": "2024",
    "license": "MIT"
  },
  "dependencies": {
    "python": ">=3.8",
    "fastapi": ">=0.68.1",
    "uvicorn": ">=0.15.0",
    "PyQt5": ">=5.15.4",
    "requests": ">=2.26.0",
    "pydantic": ">=1.8.2",
    "numpy": ">=1.21.2",
    "psutil": ">=5.8.0",
    "wmi": ">=1.5.1",
    "sounddevice": ">=0.4.3",
    "soundfile": ">=0.10.3",
    "multiprocessing-logging": ">=0.3.1",
    "python-multipart": ">=0.0.5",
    "aiofiles": ">=0.7.0",
    "python-jose[cryptography]": ">=3.3.0",
    "passlib[bcrypt]": ">=1.7.4",
    "websockets": ">=10.0",
    "aiohttp": ">=3.8.1",
    "cryptography": ">=35.0.0",
    "pyOpenSSL": ">=21.0.0",
    "service_identity": ">=21.0.0"
  },
  "deployment": {
    "production": {
      "host": "0.0.0.0",
      "port": 8082,
      "workers": 4,
      "timeout": 30,
      "ssl": false,
      "cors_origins": ["*"],
      "log_level": "info"
    },
    "streaming": {
      "host": "0.0.0.0",
      "port": 8081,
      "workers": 2,
      "timeout": 60,
      "ssl": false,
      "cors_origins": ["*"],
      "log_level": "info"
    },
    "simple": {
      "host": "0.0.0.0",
      "port": 8080,
      "workers": 1,
      "timeout": 15,
      "ssl": false,
      "cors_origins": ["*"],
      "log_level": "info"
    }
  },
  "features": {
    "web_search": true,
    "enhanced_reasoning": true,
    "real_time_data": true,
    "advanced_ai": true,
    "structured_streaming": true,
    "object_mode": true,
    "array_mode": true,
    "no_schema_mode": true,
    "creative_software_knowledge": true
  }
} 