{
  "version": 2,
  "name": "coresai-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-domain.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_PRODUCTION_API_URL": "https://your-production-backend.vercel.app",
    "REACT_APP_STREAMING_API_URL": "https://your-streaming-backend.vercel.app"
  },
  "build": {
    "env": {
      "REACT_APP_PRODUCTION_API_URL": "https://your-production-backend.vercel.app",
      "REACT_APP_STREAMING_API_URL": "https://your-streaming-backend.vercel.app"
    }
  }
} 